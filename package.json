{
  "name": "rm-nmless",
  "version": "1.2.11",
  "main": "./src/run.js",
  "author": {
    "name": "lgtome",
    "email": "seryikotenok232@gmail.com"
  },
  "description": "CLI for deleting node_modules from an old projects",
  "license": "MIT",
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^4.0.0",
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/register": "^7.17.7",
    "ava": "^4.2.0",
    "c8": "^7.11.2",
    "eslint": "^8.14.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.2",
    "mock-fs": "^5.1.2",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "sinon": "^13.0.2"
  },
  "scripts": {
    "serve": "node ./src/bin/exec",
    "test": "ava",
    "coverage": "c8 -c ./c8.json ava",
    "test:coverage": "./node_modules/.bin/nyc --reporter lcov ava -require babel-core/register \"__tests__/**/*.js\" \"!__tests__/helpers/*.js\"",
    "execute": "node ./src/bin/exec.js",
    "format": "prettier --write .",
    "prepublish": "npm run format && npm run lint",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test:m": "rm-nm --path=../../ --days=7",
    "patch": "git add . && git commit -m \" patched: $npm_package_version \" && npm version patch"
  },
  "type": "module",
  "bin": {
    "rm-nm": "./src/bin/exec.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "bin",
    "src"
  ],
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "github.com/lgtome/rm-node-modules-cli.git"
  },
  "homepage": "github.com/lgtome/rm-node-modules-cli#readme",
  "exports": {
    ".": "./"
  },
  "keywords": [
    "node_modules",
    "node_module",
    "remove",
    "useless",
    "modules",
    "remove node",
    "cli",
    "CLI"
  ],
  "dependencies": {}
}
